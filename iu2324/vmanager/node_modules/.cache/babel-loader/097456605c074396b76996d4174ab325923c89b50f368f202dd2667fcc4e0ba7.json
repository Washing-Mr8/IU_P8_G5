d_4, [_hoisted_5, _createVNode($setup[\"MemberBox\"], {\n      start: $props.vm.groups,\n      all: $setup.M.getGroups(),\n      id: \"e-groups\"\n    }, null, 8 /* PROPS */, [\"start\", \"all\"])]), _createElementVNode(\"div\", _hoisted_6, [_hoisted_7, _createVNode($setup[\"RangeBox\"], {\n      start: $props.vm.ram,\n      id: \"e-ram\",\n      min: 0,\n      max: 128,\n      units: \"Gb\"\n    }, null, 8 /* PROPS */, [\"start\"])]), _createElementVNode(\"div\", _hoisted_8, [_hoisted_9, _createVNode($setup[\"RangeBox\"], {\n      start: $props.vm.hd,\n      id: \"e-hdd\",\n      min: 0,\n      max: 4 * 1024,\n      step: 256,\n      units: \"Gb\"\n    }, null, 8 /* PROPS */, [\"start\"])]), _createCommentVNode(\" Otros elementos similares \")]), _hoisted_10], 32 /* HYDRATE_EVENTS */)]),\n\n    footer: _withCtx(() => [_createElementVNode(\"button\", {\n      onClick: _cache[1] || (_cache[1] = _withModifiers(() => $setup.setVm(), [\"prevent\"])),\n      class: \"btn btn-primary\"\n    }, _toDisplayString($props.isAdd ? 'Añadir esta VM' : `Confirmar cambios a ${$props.vm.name}`), 1 /* TEXT */)]),\n\n    _: 1 /* STABLE */\n  }, 8 /* PROPS */, [\"title\"]);\n}","map":{"version":3,"names":["class","_createElementVNode","for","type","_createBlock","$setup","ref","id","title","$props","isAdd","vm","name","body","_withCtx","onSubmit","_cache","_withModifiers","e","setVm","_hoisted_1","_hoisted_2","_hoisted_3","_createVNode","start","_hoisted_4","_hoisted_5","groups","all","M","getGroups","_hoisted_6","_hoisted_7","ram","min","max","units","_hoisted_8","_hoisted_9","hd","step","_createCommentVNode","_hoisted_10","footer","onClick"],"sources":["C:\\hlocal\\IU_P8_G5\\IU_P8_G5\\iu2324\\vmanager\\src\\components\\VmAddOrEditModal.vue"],"sourcesContent":["<script setup>\r\n\r\nimport VModal from './VModal.vue'\r\nimport RangeBox from './RangeBox.vue'\r\nimport TextBox from './TextBox.vue'\r\nimport IpBox from './IpBox.vue'\r\nimport MemberBox from './MemberBox.vue'\r\nimport SelectBox from './SelectBox.vue'\r\n\r\nimport * as M from '../model.js'\r\nimport { ref } from 'vue'\r\n\r\nconst emit = defineEmits(['add', 'edit'])\r\n\r\nconst props = defineProps({\r\n  vm: Object,\r\n  isAdd: Boolean, // otherwise, editing existing instead of adding\r\n})\r\n\r\nlet modalRef = ref(null);\r\n\r\nfunction setVm() {\r\n  const vm = props.vm;\r\n  const form = document.getElementById(\"editOrAddVm\")\r\n  const valueFor = (name) => {\r\n    const input = form.querySelector(`input[name=${name}]`)\r\n    if (!input) console.log(\"ERROR: no input for name\", name, \"in\", form)\r\n    return input.value\r\n  }\r\n\r\n  console.log(\"saving VM...\", vm, form)\r\n\r\n  // valida IP de forma manual\r\n  for (let i = 0; i < 4; i++) {\r\n    let input = form.querySelector(`input[name=e-ip-${i}]`)\r\n    let o = input.value\r\n    input.setCustomValidity(o >= 0 && o < 255 ? \"\" : \"Expected an integer between 0 and 255\")\r\n  }\r\n  const ip = [0, 1, 2, 3].map(n => valueFor(`e-ip-${n}`)).join(\".\")\r\n\r\n  // comprueba validez de todos los campos, y sobreescribe resultado\r\n  if ( ! form.checkValidity()) {\r\n    // fuerza a que se muestren los errores simulando un envío\r\n    // (pero como hay errores, no se va a enviar nada :-)\r\n    form.querySelector(\"button[type=submit]\").click()\r\n    return; \r\n  }    \r\n\r\n  // todo válido: lanza evento a padre, y cierra modal\r\n  emit(props.isAdd ? 'add' : 'edit', new M.Vm(vm.id,\r\n    valueFor(\"e-name\"),\r\n    valueFor(\"e-ram\"), valueFor(\"e-hdd\"),\r\n    valueFor(\"e-cpu\"), valueFor(\"e-cores\"),\r\n    ip,\r\n    valueFor(\"e-up\"), valueFor(\"e-down\"),\r\n    +valueFor(\"e-iso\"), M.VmState.STOPPED, vm.disk, vm.memory, \r\n    JSON.parse(valueFor(\"e-groups\"))\r\n  ))\r\n  modalRef.value.hide()    \r\n}\r\n\r\n// para que el padre pueda llamar a show (hide no debería hacer falta)\r\nfunction show() {\r\n  modalRef.value.show();\r\n}\r\ndefineExpose({ show });\r\n</script>\r\n\r\n<template>\r\n  <VModal ref=\"modalRef\" id=\"vmAddOrEditModal\"\r\n    :title=\"isAdd ? 'Añadiendo VM' : `Editando VM: ${vm.name}`\" >\r\n    <template #body>\r\n      <form id=\"editOrAddVm\" @submit.prevent=\"e => setVm()\">\r\n        <div class=\"container\">\r\n          <div class=\"mb-3\">\r\n            <label for=\"e-name\" class=\"form-label\">Nombre</label>\r\n            <TextBox :start=\"vm.name\" id=\"e-name\" />\r\n          </div>\r\n\r\n          <div class=\"mb-3\">\r\n            <label for=\"e-groups\" class=\"form-label\">Grupos</label>\r\n            <MemberBox :start=\"vm.groups\" :all=\"M.getGroups()\" id=\"e-groups\" />\r\n          </div>\r\n\r\n          <div class=\"mb-3\">\r\n            <label for=\"e-ram\" class=\"form-label\">RAM (Gb)</label>\r\n            <RangeBox :start=\"vm.ram\" id=\"e-ram\" :min=\"0\" :max=\"128\" units=\"Gb\" />\r\n          </div>\r\n\r\n          <div class=\"mb-3\">\r\n            <label for=\"e-hdd\" class=\"form-label\">HDD (Gb)</label>\r\n            <RangeBox :start=\"vm.hd\" id=\"e-hdd\" :min=\"0\" :max=\"4 * 1024\" :step=\"256\" units=\"Gb\" />\r\n          </div>\r\n\r\n          <!-- Otros elementos similares -->\r\n\r\n        </div>\r\n        <button type=\"submit\" class=\"invisible\">Submit</button>\r\n      </form>\r\n    </template>\r\n    <template #footer>\r\n      <button @click.prevent=\"() => setVm()\" class=\"btn btn-primary\">\r\n        {{ isAdd ? 'Añadir esta VM' : `Confirmar cambios a ${vm.name}` }}\r\n      </button>\r\n    </template>\r\n  </VModal>\r\n</template>\r\n\r\n<!--\r\n<template>\r\n  <VModal ref=\"modalRef\" id=\"vmAddOrEditModal\"\r\n    :title=\"isAdd ? 'Añadiendo VM' : `Editando VM: ${vm.name}`\" >\r\n    <template #body>\r\n      <form id=\"editOrAddVm\" \r\n        @submit.prevent=\"e => setVm()\">\r\n        <div class=\"container\">\r\n          <TextBox :start=\"vm.name\" id=\"e-name\" label=\"nombre\" />\r\n          <MemberBox :start=\"vm.groups\" :all=\"M.getGroups()\" id=\"e-groups\" label=\"grupos\" />\r\n          <br>\r\n          <RangeBox :start=\"vm.ram\" id=\"e-ram\" label=\"ram\" :min=\"0\" :max=\"128\" units=\"Gb\" />\r\n          <RangeBox :start=\"vm.hd\" id=\"e-hdd\" label=\"hdd\" :min=\"0\" :max=\"4 * 1024\" :step=\"256\" units=\"Gb\" />\r\n          <RangeBox :start=\"vm.cpu\" id=\"e-cpu\" label=\"cpu\" :min=\"0\" :max=\"100\" units=\"%\" />\r\n          <RangeBox :start=\"vm.cores\" id=\"e-cores\" label=\"cores\" :min=\"1\" :max=\"8\" units=\"#\" />\r\n          <br>\r\n          <IpBox :start=\"vm.ip\" id=\"e-ip\" label=\"ip\" />\r\n          <RangeBox :start=\"vm.up\" id=\"e-up\" label=\"up\" :min=\"0\" :max=\"1024 * 20\" units=\"Kbps\" />\r\n          <RangeBox :start=\"vm.down\" id=\"e-down\" label=\"down\" :min=\"0\" :max=\"1024 * 20\" units=\"Kbps\" />\r\n          <br>\r\n          <SelectBox :start=\"vm.iso\" :all=\"M.getFiles()\" id=\"e-iso\" label=\"Fichero ISO\" />\r\n        </div>\r\n        <button type=\"submit\" class=\"invisible\">Submit</button>\r\n      </form>\r\n    </template>\r\n    <template #footer>\r\n      <button @click.prevent=\"() => setVm()\" class=\"btn btn-primary\">\r\n        {{ isAdd ? 'Añadir esta VM' : `Confirmar cambios a ${vm.name}` }}\r\n      </button>\r\n    </template>\r\n  </VModal>\r\n</template>-->\r\n\r\n<style scoped>\r\n</style>\r\n"],"mappings":";;EAyEaA,KAAK,EAAC;AAAW;;EACfA,KAAK,EAAC;AAAM;gCACfC,mBAAA,CAAqD;EAA9CC,GAAG,EAAC,QAAQ;EAACF,KAAK,EAAC;GAAa,QAAM;;EAI1CA,KAAK,EAAC;AAAM;gCACfC,mBAAA,CAAuD;EAAhDC,GAAG,EAAC,UAAU;EAACF,KAAK,EAAC;GAAa,QAAM;;EAI5CA,KAAK,EAAC;AAAM;gCACfC,mBAAA,CAAsD;EAA/CC,GAAG,EAAC,OAAO;EAACF,KAAK,EAAC;GAAa,UAAQ;;EAI3CA,KAAK,EAAC;AAAM;gCACfC,mBAAA,CAAsD;EAA/CC,GAAG,EAAC,OAAO;EAACF,KAAK,EAAC;GAAa,UAAQ;iCAOlDC,mBAAA,CAAuD;EAA/CE,IAAI,EAAC,QAAQ;EAACH,KAAK,EAAC;GAAY,QAAM;;;uBA5BpDI,YAAA,CAoCSC,MAAA;IApCDC,GAAG,EAAC,UAAU;IAACC,EAAE,EAAC,kBAAkB;IACzCC,KAAK,EAAEC,MAAA,CAAAC,KAAK,oCAAoCD,MAAA,CAAAE,EAAE,CAACC,IAAI;;IAC7CC,IAAI,EAAAC,QAAA,CACb,MA0BO,C